function countDown (num) {
  let clock = setInterval(function(){
    num--;
    if (num <= 0){
      clearInterval(clock);
      console.log('Done!');
    }
    else {
      console.log(num);
    }
  }, 1000);
}

function randomGame () {
  let num = '';
  let counter = 0;
  let timer = setInterval(function(){
    num = Math.random();
    counter++;
    if (num > 0.75){
      clearInterval(timer);
      console.log(`It took ${counter} of tries`);
    }
  }, 1000);
};
